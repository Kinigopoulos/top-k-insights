{
  "name": "topk-insights",
  "homepage": "http://kinigopoulos.github.io/top-k-insights",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chart.js": "^3.5.1",
    "concurrently": "^6.2.0",
    "gh-pages": "^3.2.3",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.3",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "installBack": "cd api && npm install",
    "installAll": "npm install && npm run installBack",
    "server": "npm start --prefix api",
    "dev": "concurrently \"npm start\" \"npm run start --prefix api\"",
    "topk": "concurrently \"npm start\" \"npm run server --prefix api\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000/",
  "devDependencies": {
    "axios": "^0.21.1",
    "http-proxy-middleware": "^0.19.1",
    "stream-to-array": "^2.3.0"
  }
}
